  "scripts": {
    "build": "webpack -p --env.main --env.board --env.blockly --env.timeline",
    "build:main": "webpack -p --env.main",
    "build:board": "webpack -p --env.board",
    "build:blockly": "webpack -p --env.blockly",
    "build:timeline": "webpack -p --env.timeline",
    "watch": "webpack --watch --mode development --env.main --env.board --env.blockly --env.timeline",
    "watch:main": "webpack --watch --mode development --env.main",
    "watch:board": "webpack --watch --mode development --env.board",
    "watch:blockly": "webpack --watch --mode development --env.blockly",
    "watch:timeline": "webpack --watch --mode development --env.timeline",

    "build-new:main":     "webpack --config webpack.config.new.js         --mode production  --env.main",
    "build-new:monitor":  "webpack --config webpack.config.new.js         --mode production  --env.monitor",
    "build-new:monkey":   "webpack --config webpack.config.new.js --watch --mode production  --env.monkey",
    "watch-new:main":     "webpack --config webpack.config.new.js --watch --mode development --env.main",
    "watch-new:monitor":  "webpack --config webpack.config.new.js --watch --mode development --env.monitor",
    "watch-new:monkey":   "webpack --config webpack.config.new.js --watch --mode development --env.monkey",

    "test:single": "rm -rf tmp/ && karma start karma.conf.js --single-run --browsers Chrome",
    "test:watch": "karma start karma.conf.js --browsers Chrome",
    "docs": "./node_modules/.bin/esdoc"
  },
